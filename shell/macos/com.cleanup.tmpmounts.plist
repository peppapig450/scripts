<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN"
 "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.cleanup.tmpmounts</string>

    <key>ProgramArguments</key>
    <array>
        <string>/opt/homebrew/bin/bash Users/nick/.local/bin/cleanup-tmp-mounts</string>
    </array>

    <key>StartCalendarInterval</key>
    <dict>
        <key>Hour</key>
        <integer>4</integer>
        <key>Minute</key>
        <integer>0</integer>
    </dict>

    <key>StandardOutPath</key>
    <string>/var/log/cleanup-tmpmounts.out</string>
    <key>StandardErrorPath</key>
    <string>/var/log/cleanup-tmpmounts.err</string>
</dict>
</plist>
